{
  "name": "keepr",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:stream": "turbo run dev --filter=@keepr/stream",
    "dev:commander": "turbo run dev --filter=@keepr/commander",
    "clean": "turbo run clean",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "check": "turbo run check"
  },
  "devDependencies": {
    "@keepr/eslint": "workspace:*",
    "eslint": "catalog:cli",
    "turbo": "catalog:core",
    "typescript": "catalog:cli",
    "wrangler": "catalog:cli"
  }
}
